<toolbox_set id="toolbox" style="display: none">
    <category name="TOOLBOX_ACTION" svg="true">
        <block type="robConf_digitalin"/>
        <block type="robConf_analogin"/>
        <block type="robConf_servo"/>
        <block type="robConf_stepmotor"/>
        <block type="robConf_relay"/>
        <block type="robConf_lcd"/>
        <block type="robConf_lcdi2c"/>
        <block type="robConf_led"/>
        <block type="robConf_rgbled"/>
        <block type="robConf_buzzer"/>
        <block type="robConf_oledssd1306i2c"/>
        <block type="robConf_aifes" intask="true">
            <field name="NAME">Aifes</field>
            <field name="AIFES_NUMBER_INPUT_NEURONS">1</field>
            <field name="AIFES_NUMBER_HIDDENLAYERS_NEURONS">2</field>
            <field name="AIFES_NUMBER_OUTPUT_NEURONS">1</field>
            <field name="AIFES_FNN_LAYERS">3</field>
            <field name="AIFES_LEARNINGRATE">0.5</field>
            <field name="AIFES_MOMENTUM">0.0</field>
            <field name="AIFES_DATASET">1</field>
            <field name="AIFES_EPOCHS">1000</field>
            <field name="AIFES_MAX_WEIGHT">2</field>
            <field name="AIFES_MIN_WEIGHT">-2</field>
            <field name="AIFES_LEARNINGFUNCTION">sigmoid</field>
            <field name="AIFES_OPTIMIER">adam</field>
            <field name="AIFES_LOSS">E_mse</field>
            <field name="AIFES_WEIGHT">init_uniform</field>
        </block>
    </category>
    <category name="TOOLBOX_SENSOR" svg="true">
        <block type="robConf_digitalout"/>
        <block type="robConf_analogout"/>
        <block type="robConf_key"/>
        <block type="robConf_motion"/>
        <block type="robConf_light"/>
        <block type="robConf_infrared"/>
        <block type="robConf_temperature"/>
        <block type="robConf_humidity"/>
        <block type="robConf_drop"/>
        <block type="robConf_pulse"/>
        <block type="robConf_potentiometer"/>
        <block type="robConf_moisture"/>
        <block type="robConf_ultrasonic"/>
        <block type="robConf_rfid"/>
    </category>
</toolbox_set>